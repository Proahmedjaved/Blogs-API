 [run]
source = app
omit =
    */tests/*
    */__init__.py
    */migrations/*
    */alembic/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError:
    raise Exception

[html]
directory = coverage_html